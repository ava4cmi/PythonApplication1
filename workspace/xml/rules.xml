<?xml version="1.0" encoding="UTF-8"?>
<rules>
  <SafetyAttribute>
    <Name name="Sanitize">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Data/File Format">
      <Value value="JDL"/>
      <Value value="RSL"/>
      <Value value="ClassAdd"/>
      <Value value="Submit"/>
      <Value value="SQL"/>
      <Value value="XML"/>
      <Value value="PDF"/>
      <Value value="DOC"/>
      <Value value="XLS"/>
      <Value value="TXT"/>
      <Value value="CSV"/>
      <Value value="RTF"/>
      <Value value="HTML"/>
      <Value value="Binary"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Database Interaction">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Thirdparty Operations">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Attachment">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Tampering">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Encryption">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Transform Data">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Transfering Data">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Remote/External Operation">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Timeout Operations">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Client/Server Installation">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="User Interface">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Unexpected Error handling">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="User">
      <Value value="Administrator">
        <Metric metric="Required Privilege" score="1.0" value="Administrator"/>
        <Metric metric="Acquired Privilege Layer" score="1.0" value="Enterprise"/>
        <Metric metric="Business Impact" score="1.0" value="Critical"/>
      </Value>
      <Value value="Partially-Privileged User">
        <Metric metric="Technical Impact" score="0.9" value="High"/>
        <Metric metric="Acquired Privilege" score="0.9" value="Partially-Privileged User"/>
        <Metric metric="Acquired Privilege Layer" score="0.9" value="System"/>
        <Metric metric="Business Impact" score="0.9" value="High"/>
      </Value>
      <Value value="Regular User">
        <Metric metric="Technical Impact" score="0.6" value="Medium"/>
        <Metric metric="Acquired Privilege" score="0.7" value="Regular User"/>
        <Metric metric="Acquired Privilege Layer" score="0.7" value="Network"/>
        <Metric metric="Business Impact" score="0.6" value="Medium"/>
      </Value>
      <Value value="Guest">
        <Metric metric="Technical Impact" score="0.3" value="Low"/>
        <Metric metric="Acquired Privilege" score="0.6" value="Guest"/>
        <Metric metric="Acquired Privilege Layer" score="0.5" value="Unknown"/>
        <Metric metric="Business Impact" score="0.3" value="Low"/>
      </Value>
      <Value value="None">
        <Metric metric="Technical Impact" score="0" value="None"/>
        <Metric metric="Acquired Privilege" score="0.1" value="None"/>
        <Metric metric="Acquired Privilege Layer" score="0.5" value="Unknown"/>
        <Metric metric="Business Impact" score="0" value="None"/>
      </Value>
      <Value value="Default">
        <Metric metric="Technical Impact" score="0.6" value="Default"/>
        <Metric metric="Acquired Privilege" score="0.7" value="Default"/>
        <Metric metric="Acquired Privilege Layer" score="0.9" value="Default"/>
        <Metric metric="Business Impact" score="0.6" value="Default"/>
      </Value>
      <Value value="Unknown">
        <Metric metric="Technical Impact" score="0.5" value="Unknown"/>
        <Metric metric="Acquired Privilege" score="1.0" value="Unknown"/>
        <Metric metric="Acquired Privilege Layer" score="0.5" value="Unknown"/>
        <Metric metric="Business Impact" score="0.5" value="Unknown"/>
      </Value>
      <Value value="Not Applicable">
        <Metric metric="Technical Impact" score="1.0" value="Not Applicable"/>
        <Metric metric="Acquired Privilege" score="1.0" value="Not Applicable"/>
        <Metric metric="Acquired Privilege Layer" score="1.0" value="Not Applicable"/>
        <Metric metric="Business Impact" score="1.0" value="Not Applicable"/>
      </Value>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Owner">
      <Value value="Administrator">
        <Metric metric="Technical Impact" score="1.0" value="Critical"/>
        <Metric metric="Acquired Privilege" score="1.0" value="Administrator"/>
        <Metric metric="Acquired Privilege Layer" score="1.0" value="Enterprise"/>
        <Metric metric="Business Impact" score="1.0" value="Critical"/>
      </Value>
      <Value value="Partially-Privileged User">
        <Metric metric="Technical Impact" score="0.9" value="High"/>
        <Metric metric="Acquired Privilege" score="0.9" value="Partially-Privileged User"/>
        <Metric metric="Acquired Privilege Layer" score="0.9" value="System"/>
        <Metric metric="Business Impact" score="0.9" value="High"/>
      </Value>
      <Value value="Regular User">
        <Metric metric="Technical Impact" score="0.6" value="Medium"/>
        <Metric metric="Acquired Privilege" score="0.7" value="Regular User"/>
        <Metric metric="Acquired Privilege Layer" score="0.7" value="Network"/>
        <Metric metric="Business Impact" score="0.6" value="Medium"/>
      </Value>
      <Value value="Guest">
        <Metric metric="Technical Impact" score="0.3" value="Low"/>
        <Metric metric="Acquired Privilege" score="0.6" value="Guest"/>
        <Metric metric="Acquired Privilege Layer" score="0.5" value="Unknown"/>
        <Metric metric="Business Impact" score="0.3" value="Low"/>
      </Value>
      <Value value="None">
        <Metric metric="Technical Impact" score="0" value="None"/>
        <Metric metric="Acquired Privilege" score="0.1" value="None"/>
        <Metric metric="Acquired Privilege Layer" score="0.5" value="Unknown"/>
        <Metric metric="Business Impact" score="0" value="None"/>
      </Value>
      <Value value="Default">
        <Metric metric="Technical Impact" score="0.6" value="Default"/>
        <Metric metric="Acquired Privilege" score="0.7" value="Default"/>
        <Metric metric="Acquired Privilege Layer" score="0.9" value="Default"/>
        <Metric metric="Business Impact" score="0.6" value="Default"/>
      </Value>
      <Value value="Unknown">
        <Metric metric="Technical Impact" score="0.5" value="Unknown"/>
        <Metric metric="Acquired Privilege" score="1.0" value="Unknown"/>
        <Metric metric="Acquired Privilege Layer" score="0.5" value="Unknown"/>
        <Metric metric="Business Impact" score="0.5" value="Unknown"/>
      </Value>
      <Value value="Not Applicable">
        <Metric metric="Technical Impact" score="1.0" value="Not Applicable"/>
        <Metric metric="Acquired Privilege" score="1.0" value="Not Applicable"/>
        <Metric metric="Acquired Privilege Layer" score="1.0" value="Not Applicable"/>
        <Metric metric="Business Impact" score="1.0" value="Not Applicable"/>
      </Value>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Spoofing">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Trust">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Max/Min Operations">
      <Value value="Yes"/>
      <Value value="No"/>
    </Name>
  </SafetyAttribute>
  <SafetyAttribute>
    <Name name="Programming Language">
      <Value value="C/C++"/>
      <Value value="Java"/>
      <Value value="Visual Basic"/>
      <Value value="C#"/>
      <Value value=".NET"/>
      <Value value="Fortran"/>
      <Value value="PHP"/>
      <Value value="Python"/>
      <Value value="Bash"/>
      <Value value="Ruby"/>
      <Value value="Perl"/>
      <Value value="Javascript"/>
      <Value value="PL/SQL"/>
      <Value value="Matlab"/>
    </Name>
  </SafetyAttribute>
</rules>
